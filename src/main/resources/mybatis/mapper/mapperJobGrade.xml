<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oti.team2.jobGrade.dao.IJobGradeDao">
	<select id="selectJobGradeList" resultType="jobGrade">
		SELECT 
			JBGD_CD as "jbgdCd",
			JBGD_NM as "jbgdNm"
		FROM
			JOB_GRADE
	</select>
</mapper>